<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="stylesheet" href="{{ asset('bootstrap-4.1.3/css/bootstrap.min.css') }}">
        {% block stylesheets %}{% endblock %}
    </head>
    <body>
        <nav class="navbar navbar-expand navbar-light bg-light">
            <a class="navbar-brand" href="{{ path('app_index_index') }}">Home</a>
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link" href="{{ path('app_contact_index') }}">Contact List</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="{{ path('app_contact_add') }}">Contact Add</a>
                </li>
            </ul>
        </nav>
        <div class="container-fluid">
            {% block body %}{% endblock %}
            <hr>
            {{ date() | date('H:i:s') }}
        </div>
        <script src="{{ asset('jquery-3.3.1.min.js') }}"></script>
        <script src="{{ asset('bootstrap-4.1.3/js/bootstrap.min.js') }}"></script>
        {% block javascripts %}{% endblock %}
    </body>
</html>
